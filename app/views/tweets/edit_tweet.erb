<form action='/tweets' method="post">
  Content:
  <input type="text" name="content" value="<%=@tweet.content%>"><br>
  Create Your Tweet:

  <input type="submit" name="submit" id="submit">
  <form method="delete" action="/tweets/<%=@tweet.id%>">
    <input type="submit" value="Delete Tweet">
  </form>
  <form method="post" action="/tweets/<%=@tweet.id%>">
    <input id="hidden" type="hidden" name="_method" value="PATCH">
    <input type="submit" value="Edit Tweet">
  </form>
</form>
